{% extends "HistoireBundle::base.html.twig" %}

{% block title %}{{parent()}}{% endblock %}

{% block titre %} Histoire {% endblock %}

{% block body %}

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Titre</th>
            <td>{{ entity.titre }}</td>
        </tr>
        <tr>
            <th>Date_creation</th>
            <td>{{ entity.datecreation|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Date_modification</th>
            <td>{{ entity.datemodification|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('histoire') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('histoire_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('histoire_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}